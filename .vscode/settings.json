{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/CVS": true
  },
  "hide-files.files": [],
  "remote.extensionKind": {
    "vadimcn.vscode-lldb": [
      "workspace"
    ],
    "llvm-vs-code-extensions.vscode-clangd": [
      "workspace"
    ],
  },
  "editor.tabSize": 2,
  "files.eol": "\n",
  "files.associations": {
    "*.h": "cpp"
  },
  "[cpp]": {
    "editor.defaultFormatter": "llvm-vs-code-extensions.vscode-clangd",
  },
  "clangd.arguments": [
    "--log=verbose",
    "--clang-tidy=false",
  ],
  "clangd.enable": true,
  "C_Cpp.codeAnalysis.clangTidy.enabled": true,
  "C_Cpp.codeAnalysis.clangTidy.args": [
    "--load=${workspaceFolder}/tools/clang-tidy-plugins/header-guard.so"
  ],
  "todo-tree.general.tags": [
    "[ ]",
    "[x]",
    "BUG",
    "FIXME",
    "HACK",
    "NOTE",
    "REVIEW",
    "TODO",
    "LINT",
  ],
  "editor.rulers": [
    80
  ],
  "C_Cpp.default.compileCommands": "/workspaces/INHA_OSAP_001_T2/builddir/compile_commands.json",
  "C_Cpp.default.configurationProvider": "mesonbuild.mesonbuild",
}
